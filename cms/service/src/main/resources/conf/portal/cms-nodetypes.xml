		</propertyDefinitions>
		<childNodeDefinitions>
			<childNodeDefinition name="*" defaultPrimaryType="exo:attachments" autoCreated="false" mandatory="false" onParentVersion="VERSION" protected="false" sameNameSiblings="false">
