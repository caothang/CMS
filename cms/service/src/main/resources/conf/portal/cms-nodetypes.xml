			<propertyDefinition name="exo:passWord" requiredType="String" autoCreated="true" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
				<valueConstraints/>
			</propertyDefinition>			
				<!-- contact via email with user: gmail, yahoo, facebook... -->
			<propertyDefinition name="exo:email" requiredType="String" autoCreated="true" mandatory="false" onParentVersion="COPY" protected="false" multiple="true">
				<valueConstraints/>
			</propertyDefinition>			
			<propertyDefinition name="exo:fullName" requiredType="String" autoCreated="true" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
