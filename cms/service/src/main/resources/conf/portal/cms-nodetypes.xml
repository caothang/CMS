			<!-- contact online with user: skype, yahoo, facebook... -->
			<propertyDefinition name="exo:contactsOnline" requiredType="String" autoCreated="true" mandatory="false" onParentVersion="COPY" protected="false" multiple="true">
				<valueConstraints/>
				<defaultValues>
					<defaultValue> </defaultValue>
				</defaultValues>
			</propertyDefinition>
			<!-- contact by phone with user: telephone, mobile-phone, work-phone, home-phone ... -->
			<propertyDefinition name="exo:contactsPhone" requiredType="String" autoCreated="true" mandatory="false" onParentVersion="COPY" protected="false" multiple="true">
				<valueConstraints/>
			</propertyDefinition>			
			<!-- ip address of user -->
			<propertyDefinition name="exo:ip" requiredType="String" autoCreated="true" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
				<valueConstraints/>
				<defaultValues>
					<defaultValue></defaultValue>
				</defaultValues>
			</propertyDefinition>
			<!-- default users is buyer role is 10 -->
			<propertyDefinition name="exo:userRole" requiredType="Long" autoCreated="true" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
				<valueConstraints/>
				<defaultValues>
					<defaultValue>10</defaultValue>
				</defaultValues>
			</propertyDefinition>
			<propertyDefinition name="exo:viewNow" requiredType="String" autoCreated="true" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
				<valueConstraints/>
				<defaultValues>
					<defaultValue></defaultValue>
				</defaultValues>
			</propertyDefinition>
			<propertyDefinition name="exo:signature" requiredType="String" autoCreated="true" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
				<valueConstraints/>
				<defaultValues>
					<defaultValue> </defaultValue>
				</defaultValues>
			</propertyDefinition>
			<!-- products posted, applied for moderators or administrators -->
			<propertyDefinition name="exo:totalProduct" requiredType="Long" autoCreated="true" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
				<valueConstraints/>
				<defaultValues>
					<defaultValue>0</defaultValue>
				</defaultValues>
			</propertyDefinition>
			<!-- cai nay nham liet ke so cong viec phai lam cua moderators -->
			<propertyDefinition name="exo:jobWattingForModerator" requiredType="String" autoCreated="true" mandatory="false" onParentVersion="COPY" protected="false" multiple="false">
				<valueConstraints/>
				<defaultValues>
					<defaultValue>0</defaultValue>
				</defaultValues>
			</propertyDefinition>
			<!-- if user is moderators, the role is 1 -->
